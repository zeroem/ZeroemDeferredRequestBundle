{
  "name": "zeroem/deferred-request-bundle",
  "description": "Utilities for deferring request processing to some point in the future",
  "type": "symfony-bundle",
  "keywords": ["defer", "request", "bundle", "http"],
  "license": "MIT",
  "authors": [
    {
      "name": "Darrell Hamilton",
      "email": "darrell.noice@gmail.com"
    }
  ],

  "require": {
    "symfony/symfony": "dev-master",
    "deferred-request-persister": "*",
    "deferred-request-queue-manager": "*"
  },

  "require-dev": {
    "deferred-request-dev-package": "*"
  },

  "suggest": {
    "zeroem/deferred-request-orm-persister-bundle": "A virtual package providing an implementation of deferred-request-persister.",
    "zeroem/deferred-request-rabbitmq-queue-bundle": "A virtual package providing an implementation of deferred-request-queue-manager."
  },

  "autoload": {
    "psr-0": {
      "Zeroem\\DeferredRequestBundle": ""
    }
  },

  "target-dir": "Zeroem/DeferredRequestBundle",

  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "deferred-request-dev-package",
	"version": "0.0.1",
	"provide": {
	  "deferred-request-persister": "*",
	  "deferred-request-queue-manager": "*"
	},
	"dist": {
	  "type": "file",
	  "url": "http://www.example.org/"
	}
      }
    }
  ]
}
